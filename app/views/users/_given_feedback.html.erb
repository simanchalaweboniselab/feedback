<table class="table table-bordered">
  <colgroup>
    <col class="span3">
    <col class="span3">
    <col class="span3">
  </colgroup>
  <% if feedbacks.length == 0 %>
      <caption>
        <h2>Feedback not Given!</h2>
        <%= link_to image_tag('/assets/previous.png', :height => '20', :width => '20'), nil, :date => (begin_date.to_date - 1.day), :remote => true, :id => "previous-user-given-search" %>
        <%= begin_date.nil? ? "" : "( #{begin_date} to #{end_date})" %>
        <%= link_to image_tag('/assets/next.png', :height => '20', :width => '20'), nil, :date => (end_date.to_date + 1.day), :remote => true, :id => "next-user-given-search" %>
      </caption>
      <thead>
      </thead>
  <% else %>
      <caption>
        <h2> Given Feedback List</h2>
        <%= link_to image_tag('/assets/previous.png', :height => '20', :width => '20'), nil, :date => (begin_date.to_date - 1.day), :remote => true, :id => "previous-user-given-search" %>
        <%= begin_date.nil? ? "" : "( #{begin_date} to #{end_date})" %>
        <%= link_to image_tag('/assets/next.png', :height => '20', :width => '20'), nil, :date => (end_date.to_date + 1.day), :remote => true, :id => "next-user-given-search" %>
      </caption>
      <thead>
      <tr style = "background-color: #A2AFCC;">
        <th>Weboniser</th>
        <th>Date</th>
        <th>Feedback</th>
      </tr>
      </thead>
      <% feedbacks.each do |feedback| %>
          <tr>
            <td><%= user_name(feedback.to_id) %></td>
            <td><%= date(feedback.updated_at) %></td>
            <td><%= feedback.feedback %></td>
          </tr>
      <% end %>
  <% end %>
</table>
<%= page_navigation_links feedbacks %>