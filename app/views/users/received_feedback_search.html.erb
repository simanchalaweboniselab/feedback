<div class="pull-right">
  <input type="text" data-behaviour='datepicker' id="datepicker" >
  <%= link_to "search", nil, :class => "btn btn-info", :style => "margin-top:-9px; margin-left:-4px;", :remote => true, :id => "user-received-search" %>
</div>
<div id="user_received_feedback">
  <%= render :partial => "received_feedback", :locals => {:users => @users, :feedbacks => @feedbacks, :end_date => @end_date.strftime("%d-%b-%Y"), :begin_date => @begin_date.strftime("%d-%b-%Y")} %>
</div>