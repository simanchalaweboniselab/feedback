<table class="table table-bordered">
  <colgroup>
    <col class="span2">
    <col class="span3">
    <col class="span3">
    <col class="span3">
  </colgroup>
  <caption>
    <% if feedbacks.length == 0 %>
        <h2> Not given any feedback this week</h2><%= begin_date.nil? ? "" : "( #{begin_date} to #{end_date})" %>
    <% else %>
        <h2>Given Feedback List <%= user.name %> </h2><%= begin_date.nil? ? "" : "( #{begin_date} to #{end_date})" %>

        </caption>
        <thead>
        <tr style = "background-color: #A2AFCC;">
          <th>Name</th>
          <th>Assigned Date</th>
          <th>Given Date</th>
          <th>Feedback</th>
        </tr>
        </thead>
        <% feedbacks.each do |feedback| %>
            <tr>
              <td><%= user_name(feedback.to_id) %></td>
              <td><%= date(feedback.created_at) %></td>
              <td><%= date(feedback.updated_at) if !feedback.feedback.to_s.empty? %></td>
              <td><%= feedback.feedback if !feedback.feedback.to_s.empty? %></td>
            </tr>
        <% end %>
    <% end %>
    </table>
<%= link_to "Back", admin_users_path, :class => "btn" %>
<%= page_navigation_links feedbacks %>